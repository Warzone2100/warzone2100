#: po/custom/fromJson.txt:848
msgid "Machinegun"
msgstr "Gépfegyver"

#: data/mp/messages/strings/names.txt:275
msgid "VTOL Flak Cannon"
msgstr "VTOL légelhárító löveg"

#: data/mp/messages/strings/names.txt:293
#: po/custom/fromJson.txt:332
msgid "Collective Wall"
