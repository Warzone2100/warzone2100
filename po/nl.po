msgstr "Vijandelijke LZ"

#: data/base/messages/strings/genstrings.txt:23
msgid "Enemy Base"
msgstr "Vijandelijke basis"

#: data/base/messages/strings/genstrings.txt:27
msgid "Incoming transmission"
msgstr "Binnenkomende uitzending"

#: data/base/messages/strings/genstrings.txt:30
msgid "Incoming intelligence report."
msgstr "Binnenkomende inlichtingen"

#: data/base/messages/strings/genstrings.txt:33
msgid "Nuclear Reactor"
msgstr "Nucleaire reactor"

#: data/base/messages/strings/names.txt:12
#: data/mp/messages/strings/names.txt:12
msgid "New Design"
msgstr "Nieuw ontwerp"

#: data/base/messages/strings/names.txt:103
#: data/mp/messages/strings/names.txt:160
msgid "Cyborg Cannon"
msgstr "Cyborg-kanon"

#: data/base/messages/strings/names.txt:149
#: data/base/messages/strings/names.txt:182
#: data/mp/messages/strings/names.txt:198
#: data/mp/messages/strings/names.txt:218
msgid "Demolish"
msgstr "Slopen"

#: data/base/messages/strings/names.txt:156
#: po/custom/fromJson.txt:479
msgid "Naval"
msgstr "Naval"

#: data/base/messages/strings/names.txt:183
#: data/base/messages/strings/names.txt:558
#: data/mp/messages/strings/names.txt:219
#: data/mp/messages/strings/names.txt:708
msgid "Repair Center"
msgstr "Reparatiecentrum"

#: data/base/messages/strings/names.txt:190
#: data/base/messages/strings/names.txt:307
#: data/mp/messages/strings/names.txt:388
msgid "Tornado AA Flak Site"
msgstr "Tornado AA Luchtgeschuts Platform"

